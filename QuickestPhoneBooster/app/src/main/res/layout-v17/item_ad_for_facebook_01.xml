<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res-auto" app:orientation="vertical" app:id="@+id/container" app:background="@drawable/bg_white_bottom_corner_2" app:layout_width="match_parent" app:layout_height="wrap_content">
